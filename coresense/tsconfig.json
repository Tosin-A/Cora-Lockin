{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@screens/*": [
        "./screens/*"
      ],
      "@stores/*": [
        "./stores/*"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@types/*": [
        "./types/*"
      ],
      "@constants/*": [
        "./constants/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx"
  ]
}






